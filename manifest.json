{
  "name": "Accounting Example",
  "version": "1.0",
  "description": "Accounting Extension!",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    "./*"
  ]
}
